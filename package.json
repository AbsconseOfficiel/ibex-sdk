{
  "name": "@absconse/ibex-sdk",
  "version": "1.1.1",
  "description": "SDK optimisé pour l'intégration des services IBEX - Architecture hybride (API REST + WebSocket) avec 90% de réduction de requêtes",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "CONTRIBUTING.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run type-check",
    "prepack": "npm run build",
    "security-check": "node scripts/security-check.js"
  },
  "keywords": [
    "ibex",
    "auth",
    "webauthn",
    "passkeys",
    "react",
    "typescript",
    "sdk",
    "blockchain",
    "safe",
    "eure",
    "crypto",
    "wallet",
    "kyc",
    "identity",
    "simplified",
    "unified",
    "hooks",
    "easy-to-use"
  ],
  "author": {
    "name": "Absconse, Dylan ENJOLVIN",
    "url": "https://www.linkedin.com/in/dylanenjolvin/"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbsconseOfficiel/ibex-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/AbsconseOfficiel/ibex-sdk/issues"
  },
  "homepage": "https://github.com/AbsconseOfficiel/ibex-sdk#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.4.4",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/AbsconseOfficiel"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
